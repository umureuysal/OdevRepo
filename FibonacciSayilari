import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Bir sayÄ± giriniz: ");
        int num = 10;
        int a = 0, b = 1, c;
        for (int i = 0; i <= num; i++) {
            c=a+b;
            System.out.println(a+"+"+b+"="+c);
            a=b;
            b=c;
        }
    }

}
